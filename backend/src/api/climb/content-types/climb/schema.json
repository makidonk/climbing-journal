{
  "kind": "collectionType",
  "collectionName": "climbs",
  "info": {
    "singularName": "climb",
    "pluralName": "climbs",
    "displayName": "climb"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "color": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "grön",
        "blå",
        "röd",
        "svart",
        "vit"
      ]
    },
    "climbType": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "sloper",
        "slab",
        "overhang",
        "rope",
        "idk"
      ]
    },
    "description": {
      "type": "richtext",
      "default": "Hur gick det?"
    },
    "media_items": {
      "type": "component",
      "component": "media.media-list",
      "repeatable": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "place": {
      "type": "string"
    },
    "difficulty": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "easy",
        "medium",
        "hard"
      ]
    }
  }
}
